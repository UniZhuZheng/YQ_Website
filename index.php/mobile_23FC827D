		var sedleft=document.getElementById("sed_left");
		var sedright=document.getElementById("sed_right");
		var mask=document.getElementById("masks");
		var mask_ul=mask.getElementsByTagName("ul")[0];
		var mask_li=mask.getElementsByTagName("ul")[0].getElementsByTagName("li");
		sedleft.onclick=function(){
			autoscroll(0);
		
		}
		sed_right.onclick=function(){
			rightscroll(0);
		}
		function autoscroll(num){
			num+=20;
			if(num<=1000)
				{	mask_li[0].style.marginLeft=-num+"px";
					window.setTimeout(function(){autoscroll(num);},20);
				}
			else
				{
					mask_ul.appendChild(mask_li[0]);
					mask_li[mask_li.length-1].style.marginLeft="0px";
					num=0;
				}
			
		}
		function rightscroll(num)
			{
				if(num==0)
				{
					mask_ul.insertBefore(mask_li[mask_li.length-1],mask_li[0]);
					mask_li[0].style.marginLeft="-1000px";
				}
				num+=20;
				if(num<1000)
				{
					mask_li[0].style.marginLeft=(-980+num)+"px";
					window.setTimeout(function(){rightscroll(num);},20);
				}
				
			
			}
        libtn.click(function(){
            var index=$(this).parent().index()
            $(".list-mobel li").removeClass("cus")
            $(this).parent().addClass("cus")
            $(".leftsay").html(array[index].tit).addClass("animated flipInX");
            $(".data-say").find("img").attr("src",array[index].data).parent().addClass("animated flipInX");
            var selid=$(".incse")
            selid.stop("true").animate({"marginLeft":index*200+"px"},500)
        })
    })
    var qs = (function() {
        var argName, args, pair, pairs, pos, query, value, _i, _len;
        args = {};
        query = window.location.search.substring(1);
        pairs = query.split('&');
        for (_i = 0, _len = pairs.length; _i < _len; _i++) {
            pair = pairs[_i];
            pos = pair.indexOf('=');
            if (pos === -1) {
                continue;
            }
            argName = pair.substr(0, pos);
            value = pair.substr(pos + 1);
            value = decodeURIComponent(value);
            args[argName] = value;
        }
        return args;
    })()
    function  scls(){
        var scrollnum=qs.id;
        var top,tit=$(".tit-gy-one");
        switch(scrollnum){
            case "0":
                top=tit.eq(0).offset().top-50;
                break;
            case "1":
                top=tit.eq(1).offset().top-50;
                break;
            case "2":
                top=tit.eq(2).offset().top-50;
                break;
            case "3":
                top=tit.eq(3).offset().top-50;
                break;
        }
        $('body,html').animate({scrollTop:top},800)
    }
</script>
