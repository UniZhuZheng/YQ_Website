    var peopleli=document.getElementById("people").getElementsByTagName("li")
    function team(num){
        num+=20;
        if(num<=316){
            peopleli[0].style.marginLeft=-num+"px";
            window.setTimeout(function(){team(num)},20);
        }
        else{
            document.getElementById("people").appendChild(peopleli[0]);
            peopleli[peopleli.length-1].style.marginLeft="0px";
            num=0;
        }
    }
    function leftteam(num){
        if(num==0){
            document.getElementById("people").insertBefore(peopleli[peopleli.length-1],peopleli[0]);
            peopleli[0].style.marginLeft="-316px";
        }
        num+=20;
        if(num<=316){
            peopleli[0].style.marginLeft=(-300+num)+"px";
            window.setTimeout(function(){leftteam(num);},20);
        }
    }


    $(".conybtn a").click(function(){
        $('body,html').animate({scrollTop:$(".fuzc").offset().top},500)
    })
    function boxmove(){
        // if(bool==1){
        var hei =document.documentElement.scrollTop || document.body.scrollTop;
        if(hei>300){
            $(".piclic").animate({"marginTop":"0px","opacity":"1"},800,function(){
                $(".desip").animate({"marginTop":"0px","opacity":"1"},800)
            })
        }
        if(hei>600){
            play(0);
        }
        if(hei>800){
            play(1);
        }
        if(hei>1000){
            play(2);
        }
        if(hei>1200){
            play(3);
        }
        if(hei>1400){
            play(4)
        }
        if(hei>1665){
            $(".picbox").animate({"marginLeft":"0px","opacity":1},2000)
        }
        else
            return false;
        //console.log(j)
        function play(j){
            var newdom=$(".servlist li");
            newdom.eq(j).find(".modom").animate({
                "marginTop":"0px", "opacity":"1"
            },300)
            newdom.eq(j).find("span").animate({
                "marginTop":"0px", "opacity":"1"
            },300,function(){
                $(this).parents("li").find("i").addClass("dol");
            })
            /*newdom.eq(j).stop().find(".modom").stop().animate({
             "marginTop":"0px","opacity":"1"
             },200,function(){
